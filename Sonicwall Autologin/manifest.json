{
  "manifest_version": 2,
  "name": "Sonicwall Auto Login Extension",
  "version": "0.1",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://127.0.0.1/*"
      ],
      "js": [
        "jquery.js",
        "login.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ]
}